data = {}
n = int(input("Enter the number of data- "))
for i in range(1,n+1):
    da = {}
    name = input("Enter the name- ")
    std = input("Enter your standard- ")
    roll_no = int(input("Enter the roll number- "))
    da["Name"]=name
    da["standard"]=std
    da["Roll number"]=roll_no
    data["Child"+str(i)]=da
print(data)
